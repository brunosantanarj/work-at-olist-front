{
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 7
	},
	"extends": [
		"airbnb"
	],
	"globals": {
		"document": true,
		"window": true,
		"process": true,
		"commonjs": true
	},
	"env": {
		"browser": true,
		"node": true,
		"jasmine": true
	},
	"rules": {
    "import/prefer-default-export": 0,
    "no-unused-expressions": ["error", { "allowTernary": true }]
	}
}
